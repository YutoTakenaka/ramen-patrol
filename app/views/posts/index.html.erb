<%= render "shared/header" %>

<div class="contents">

  <% @posts.each do |post| %>
    <%= render partial: "posts/ramen", locals: {post: post} %>
  <% end %>
  
</div>
  
<%= link_to new_post_path , class: 'post-btn' do %>
  <span class='post-btn-text'>パトロールする</span>
  <%= image_tag 'new.png' , size: '185x50' ,class: "post-btn-icon" %>
<% end %>

<%= render "shared/footer" %>

